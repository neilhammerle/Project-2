  <!-- Sign Up Form -->
  <form id="signup-form" class="form-login card " toggle="off" style="display: none;">
    <div class="row">
      <div class="col-3"></div>
      <div class="col-6">
        <div class="text-center mb-4">
          <h1 class="h3 mb-3 font-weight-normal">Sign up</h1>
          <p></a>
          </p>
        </div>

        <div class="form-label-group">
          <input type="name" id="signupInputFullName" class="form-control" placeholder="John Smith" required
            autofocus>
